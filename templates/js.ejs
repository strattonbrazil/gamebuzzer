
<script src='/static/js/jquery-2.2.2.min.js'></script>
<script src='/static/js/jquery.jrumble.1.3.min.js'></script>

<script> // this is specifically for the logo, it serves no other purpose
$(function(){
    $('.logo').jrumble({speed:0});

    var demoStart = function(){
       $('.logo').trigger('startRumble');
       $('.logo').css('textShadow','#FF0 4px 4px,#FF0 -4px -4px');
       setTimeout(demoStop, 300);
    };
    var demoStop = function(){
       $('.logo').trigger('stopRumble');
       $('.logo').css('textShadow','');
       setTimeout(demoStart, 3000);
    };
    demoStart();
});
</script>
<script src="https://cdn.socket.io/socket.io-1.2.0.js"></script>
<script src="http://code.jquery.com/jquery-1.11.1.js"></script>
<script>
  var socket = io();
  $('form').submit(function(){
    socket.emit('chat message', $('#m').val());
    $('#m').val('');
    return false;
  });
  socket.on('chat message', function(msg){
    $('#messages').append($('<li>').text(msg));
  });
</script>
